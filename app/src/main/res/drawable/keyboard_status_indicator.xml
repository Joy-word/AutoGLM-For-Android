<?xml version="1.0" encoding="utf-8"?>
<!--
    Status indicator drawable for AutoGLM Keyboard.
    Shows a green circle indicating the keyboard is active.
    
    Requirements: 2.4
-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    
    <solid android:color="@color/status_running" />
    
    <size
        android:width="8dp"
        android:height="8dp" />
        
</shape>
